	<queryString>
		<![CDATA[SELECT trunc(ACCEPT_DATE) as ad,  
SUM(case  when ACCEPT_STATUS = '0000'  then 1 else 0 end) as success, 
SUM(case when ACCEPT_STATUS <> '0000' and
	ACCEPT_STATUS IS NOT NULL then 1 else 0 end
) as failure, 
SUM(case when ACCEPT_STATUS IS NOT NULL then (1 * TOTAL_SEG) else 0 end) as total
FROM  "SMS_RECORD"  s1
WHERE SMS_TYPE = 'MT' and ( ($P{selectionType}='121' AND s1."SYS_ID" = $P{sysId} AND s1."OA" = $P{sourceId} )
OR ($P{selectionType}='12A'AND s1."SYS_ID" = $P{sysId} )
OR ($P{selectionType}='A2N' ) )
GROUP BY trunc(ACCEPT_DATE)
HAVING trunc(ACCEPT_DATE)
BETWEEN  $P{sDate} AND  $P{eDate}  + 0.999988426 
ORDER BY ad]]>
	</queryString>
