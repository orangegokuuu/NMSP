	<queryString>
		<![CDATA[SELECT  trunc(s1.ACCEPT_DATE , 'HH24') as ad,   to_char(s1.ACCEPT_DATE , 'HH24') as hours,
SUM(CASE WHEN s2.DELIVER_STATUS like 'DELIVRD%' or s2.DELIVER_STATUS like 'ACCEPTD%' THEN 1 ELSE 0 END) as success, 
SUM(CASE WHEN s2.DELIVER_STATUS like 'ENROUTE%' or s2.DELIVER_STATUS like 'EXPIRED%' or s2.DELIVER_STATUS like 'DELETED%' or s2.DELIVER_STATUS 
	like 'UNDELIV%' or s2.DELIVER_STATUS like 'REJECTD%' or s2.DELIVER_STATUS like 'UNKNOWN%' OR s2.DELIVER_STATUS IS NULL THEN 1 ELSE 0 END
) as failure, 
SUM(CASE WHEN s2.DELIVER_STATUS IS NOT NULL OR s2.DELIVER_STATUS IS NULL THEN 1 ELSE 0 END) as total
from SMS_RECORD s1
left JOIN SMS_RECORD_SUB s2
   ON s1.WS_MSG_ID = s2.WS_MSG_ID
WHERE  s1.DR_FLAG = 1 AND (($P{selectionType}='121' AND  s1.SYS_ID = $P{sysId} AND s1.OA = $P{sourceId} ) 
OR ($P{selectionType}='12A'AND  s1.SYS_ID = $P{sysId} ) 
OR ($P{selectionType}='A2N' ) )
GROUP BY  trunc(s1.ACCEPT_DATE , 'HH24'),   to_char(s1.ACCEPT_DATE , 'HH24')
HAVING trunc(s1.ACCEPT_DATE , 'HH24')
BETWEEN  $P{sDate} AND $P{eDate}  + 0.999988426 
ORDER BY ad]]>
	</queryString>
