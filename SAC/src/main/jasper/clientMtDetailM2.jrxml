	<queryString>
		<![CDATA[SELECT  trunc(s1.ACCEPT_DATE, 'MONTH') as ad,
s1.SYS_ID as SYS_ID,
SUM(case when s1.ACCEPT_STATUS = '0000'  then 1 else 0 end) as success, 
SUM(case when s1.ACCEPT_STATUS <> '0000' then 1 else 0 end
) as failure, 
SUM(case when s1.ACCEPT_STATUS IS NOT NULL OR s1.ACCEPT_STATUS IS NULL then 1 else 0 end) as total
FROM  "SMS_RECORD" s1 
left JOIN "CONTENT_PROVIDER" s2
    ON s1.SYS_ID = s2.CP_ID 
WHERE s1.SMS_TYPE = 'MT' 
GROUP BY trunc(ACCEPT_DATE, 'MONTH'), s1.SYS_ID
HAVING trunc(ACCEPT_DATE, 'MONTH')
BETWEEN  $P{sDate} AND  $P{eDate}  + 0.999988426 
ORDER BY ad]]>
	</queryString>
