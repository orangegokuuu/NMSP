	<queryString>
		<![CDATA[SELECT  trunc(ACCEPT_DATE , 'HH24') as ad,   to_char(ACCEPT_DATE , 'HH24') as hours,
SUM(case  when ACCEPT_STATUS = '0000'  then 1 else 0 end) as success, 
SUM(case when ACCEPT_STATUS <> '0000'  then 1 else 0 end) as failure, 
SUM(case when ACCEPT_STATUS IS NOT NULL OR ACCEPT_STATUS Is NULL then (1 * TOTAL_SEG) else 0 end) as total
FROM  "SMS_RECORD"  s1
WHERE  SMS_TYPE = 'MO' and ( ($P{selectionType}='121' AND  s1."SYS_ID" = $P{sysId} AND s1."OA" = $P{sourceId} )
OR ($P{selectionType}='12A'AND s1."SYS_ID" = $P{sysId} )
OR ($P{selectionType}='A2N' ))
GROUP BY trunc(ACCEPT_DATE , 'HH24'), to_char(ACCEPT_DATE , 'HH24')
HAVING trunc(ACCEPT_DATE, 'HH24')
BETWEEN  $P{sDate} AND $P{eDate}  + 0.999988426 
ORDER BY ad]]>
	</queryString>
